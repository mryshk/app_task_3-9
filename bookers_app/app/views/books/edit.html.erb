<h1>Editing Book</h1>
<%= form_with model:@list,url:"/books/#{@list.id}",local:true do |f| %>
<div class="field">
<h4>Title</h4>
<%= f.text_field :title %>
</div>
<div class="field">
<h4>Body</h4>
<%= f.text_area :body %>
</div>
<div class="action">
<%= f.submit 'Update' %>
</div>
<% end %>
<span><%= link_to "Show",book_path(@list.id) %></span>
<span>|</span>
<span><%= link_to "Back",books_path %></span>
